{
  "name": "universal-starter",
  "version": "2.0.0",
  "description": "Angular 2 Universal starter kit by @AngularClass",
  "repository": {
    "type": "git",
    "url": "https://github.com/angular/universal-starter.git"
  },
  "scripts": {
    "rimraf": "rimraf",
    "tslint": "tslint",
    "typedoc": "typedoc",
    "typings": "typings",
    "webpack": "webpack",
    "concurrently": "concurrently",
    "pm2": "pm2",
    "nodemon": "nodemon",
    "clean": "npm cache clean && npm run rimraf -- node_modules typings coverage dist",
    "clean:dist": "npm run rimraf -- dist",
    "preclean:install": "npm run clean",
    "clean:install": "npm set progress=false && npm install",
    "preclean:start": "npm run clean",
    "clean:start": "npm start",
    "watch": "webpack --watch",
    "prebuild": "npm run clean:dist",
    "build": "webpack",
    "prebuild:prod": "npm run clean:dist",
    "build:prod": "cross-env NODE_ENV=production webpack",
    "server:dev": "nodemon dist/server/server.js",
    "server:prod": "pm2 start ecosystem.json --env production",
    "start:dev": "concurrently \"npm run watch\" \"npm run server:dev\"",
    "start": "npm run server:prod",
    "restart": "npm stop && npm start",
    "stop": "pm2 delete ecosystem.json",
    "deploy:setup": "pm2 deploy ecosystem.json production setup",
    "deploy": "pm2 deploy ecosystem.json production",
    "predebug": "npm run build",
    "debug": "node --debug-brk dist/server/server.js",
    "status": "pm2 show $(node -e \"console.log(require('./ecosystem.json').name)\")",
    "logs": "pm2 logs $(node -e \"console.log(require('./ecosystem.json').name)\")",
    "postinstall": "npm run typings -- install"
  },
  "license": "MIT",
  "contributors": [
    "AngularClass <hello@angularclass.com>",
    "PatrickJS <patrick@angularclass.com>",
    "Jeff Whelpley <jeff@gethuman.com>",
    "Jeff Cross <crossj@google.com>"
  ],
